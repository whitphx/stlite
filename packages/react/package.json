{
  "name": "@stlite/react",
  "version": "0.1.0",
  "description": "React component for Stlite, a serverless Streamlit in browser",
  "license": "MIT",
  "author": "Yuichiro Tachibana <yuichiro@whitphx.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitphx/stlite.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/whitphx/stlite/issues"
  },
  "main": "./dist/stlite-react.umd.cjs",
  "module": "./dist/stlite-react.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stlite-react.mjs",
      "require": "./dist/stlite-react.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": ["*.css"],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@stlite/browser": "^0.90.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "@stlite/browser": "^0.90.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}