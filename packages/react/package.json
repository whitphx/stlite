{
  "name": "@stlite/react",
  "version": "0.1.0",
  "description": "React wrapper for stlite",
  "main": "./dist/stlite-react.umd.cjs",
  "module": "./dist/stlite-react.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "Yuichiro Tachibana <yuichiro@whitphx.net>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stlite/browser": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stlite/browser": "workspace:^",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stlite-react.mjs",
      "require": "./dist/stlite-react.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": [
    "*.css"
  ]
}