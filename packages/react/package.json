{
  "name": "@stlite/react",
  "version": "0.1.0",
  "description": "A React wrapper for @stlite/browser to embed Streamlit apps in React applications.",
  "keywords": [
    "streamlit",
    "stlite",
    "react",
    "wasm",
    "pyodide",
    "browser"
  ],
  "homepage": "https://github.com/whitphx/stlite/tree/main/packages/react#readme",
  "license": "MIT",
  "author": "Yuichiro Tachibana <yuichiro.tachibana.phx@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitphx/stlite.git",
    "directory": "packages/react"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stlite-react.mjs",
      "require": "./dist/stlite-react.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/stlite-react.umd.cjs",
  "module": "./dist/stlite-react.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@stlite/browser": "workspace:*",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "sass": "^1.72.0"
  },
  "publishConfig": {
    "access": "public"
  }
}