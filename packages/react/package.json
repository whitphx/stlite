{
  "name": "@stlite/react",
  "version": "0.1.0",
  "description": "Stlite React component for embedding Streamlit apps in browsers",
  "homepage": "https://github.com/whitphx/stlite/tree/main/packages/react#readme",
  "bugs": {
    "url": "https://github.com/whitphx/stlite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitphx/stlite.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "Yuichiro Tachibana <yuichiro@whitphx.net>",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/stlite-react.umd.cjs",
  "module": "./dist/stlite-react.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stlite-react.mjs",
      "require": "./dist/stlite-react.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@stlite/browser": ">=0.76.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@stlite/browser": "workspace:*",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "to-toml": "^1.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}