{
  "name": "@stlite/sharing-editor",
  "version": "0.90.3",
  "private": true,
  "homepage": "https://edit.share.stlite.net/",
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@stlite/common": "workspace:^",
    "@stlite/sharing-common": "workspace:^",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "classnames": "^2.3.2",
    "mime": "^4.0.1",
    "monaco-editor": "^0.52.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "~5.4.0",
    "react-router-dom": "^7.9.6",
    "superstruct": "^2.0.2",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start:web": "cross-env SHARING_APP_URL=http://localhost:3000/ vite",
    "start:sampleapps": "chokidar 'public/samples/**/*' -c \"yarn build:sampleapps\" --initial",
    "start": "run-p start:web start:sampleapps",
    "build:web": "tsc -b && vite build",
    "build:sampleapps": "tsx bin/gen-sample-app-manifests-json.mts",
    "build": "yarn build:sampleapps && yarn build:web",
    "test": "vitest",
    "fix:eslint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write .",
    "check:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "check:prettier": "prettier --check ."
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jsdom": "^21.1.7",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^20.0.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.90.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.5",
    "vitest": "^4.0.15"
  }
}
