{
  "name": "@stlite/react",
  "version": "0.0.1",
  "type": "module",
  "description": "Stlite React library",
  "keywords": [
    "stlite",
    "streamlit",
    "pyodide",
    "webassembly",
    "wasm"
  ],
  "bugs": {
    "url": "https://github.com/whitphx/stlite/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Yuichiro Tachibana (Tsuchiya)",
    "url": "https://www.whitphx.info/"
  },
  "files": [
    "build/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./build/stlite.d.ts",
        "default": "./build/stlite.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitphx/stlite.git",
    "directory": "packages/react"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/js": "^9.32.0",
    "@stlite/common": "workspace:^",
    "@stlite/common-react": "workspace:^",
    "@stlite/devutils": "workspace:^",
    "@stlite/kernel": "workspace:^",
    "@streamlit/app": "workspace:^",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.1",
    "codedent": "^0.1.2",
    "eslint": "^9.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.90.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.8",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "test": "vitest",
    "fix:eslint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write .",
    "check:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "check:prettier": "prettier --check ."
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
