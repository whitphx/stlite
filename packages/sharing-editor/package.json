{
  "name": "@stlite/sharing-editor",
  "version": "0.14.0",
  "private": true,
  "homepage": "https://edit.share.stlite.net/",
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@stlite/sharing-common": "^0.14.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.7",
    "@types/react-dom": "^17.0.5",
    "classnames": "^2.3.2",
    "mime": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "4.0.3",
    "sass": "^1.54.9",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:web": "cross-env PORT=3030 react-scripts start",
    "start:sampleapps": "chokidar 'public/samples/**/*' -c \"if [ '{event}' = 'add' -o '{event}' = 'unlink' ]; then yarn build:sampleapps; fi;\" --initial",
    "start": "concurrently \"yarn start:web\" \"yarn start:sampleapps\"",
    "build:web": "react-scripts build",
    "build:sampleapps": "ts-node bin/gen-sample-app-manifests-json.ts",
    "build": "yarn build:sampleapps && yarn build:web",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/mime": "^3.0.1",
    "raw-loader": "^4.0.2",
    "runtypes": "^6.6.0"
  }
}
