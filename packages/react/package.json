{
  "name": "@stlite/react",
  "version": "0.1.0",
  "description": "Stlite React component for embedding Streamlit apps in web pages",
  "keywords": [
    "streamlit",
    "stlite",
    "pyodide",
    "webassembly",
    "wasm",
    "react"
  ],
  "homepage": "https://github.com/whitphx/stlite",
  "bugs": {
    "url": "https://github.com/whitphx/stlite/issues"
  },
  "license": "MIT",
  "author": "Yuichiro Tachibana <yuichiro@whitphx.net>",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whitphx/stlite.git",
    "directory": "packages/react"
  },
  "type": "module",
  "main": "./dist/stlite-react.umd.cjs",
  "module": "./dist/stlite-react.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stlite-react.mjs",
      "require": "./dist/stlite-react.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {},
  "peerDependencies": {
    "@stlite/browser": "0.81.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stlite/browser": "0.81.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.4.0"
  }
}